<apex:page standardController="Quote" renderAs="PDF" applyBodyTag="false" extensions="QuotePDFControllerExtension">

    <head>
        <style>
            * {
                font-family: SansSerif;
            }

            .headerTD {
                padding: 0px;
                margin: 0px;
                vertical-align: bottom;
            }

            .header-left-section {
                text-align: left;
                margin-top: 0px;
                margin-bottom: 0px;
                white-space: nowrap;
            }

            .header-right-section {
                text-align: right;
                margin-top: 0px;
                margin-bottom: 0px;
                white-space: nowrap;
            }

            .table-section {
                text-align: left;
                margin-top: 0px;
                margin-bottom: 0px;
                font: bold 13px;
                line-height: 16px;
            }

            .div-second-page {
                margin: 50px 0px 0px 6px;
                padding: 0px;
                border: none;
                width: 865px;
            }

            .td-second-page {
                padding: 0px;
                margin-top: 0px;
                margin-bottom: 0px;
                border-right: 1px solid #000000;
                border-top: 1px solid #000000;
            }

            .td-header {
                background: #e7e6e6;
            }

            .td-bottom-second-page {
                border-bottom: 1px solid #000000;
            }

            .td-top-second-page {
                border-top: 1px solid #000000;
            }

            .td-left-second-page {
                border-left: 1px solid #000000;
            }

            .td-empty-second-page {
                padding: 0px;
                margin-top: 0px;
                margin-bottom: 0px;
            }

            .p-second-page {
                text-align: center;
                padding: 0px 3px;
                margin-top: 0px;
                margin-bottom: 0px;
                font: 15px;
                line-height: 16px;
            }

            .p-bold-second-page {
                font: bold 15px;
                line-height: 16px;
            }
            .text-align-left {
                text-align: left;
            }
        </style>
