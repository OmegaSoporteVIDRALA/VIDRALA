<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bottle_Validation_Rule</fullName>
    <active>false</active>
    <errorConditionFormula>AND
(
ISPICKVAL( New_Bottle__c , &quot;Yes&quot; ),
ISBLANK(Enter_Previous_Bottle_Code__c)
)</errorConditionFormula>
    <errorDisplayField>Enter_Previous_Bottle_Code__c</errorDisplayField>
    <errorMessage>Please add as a value to the &quot;Enter Previous Bottle Code&quot; field</errorMessage>
</ValidationRule>
