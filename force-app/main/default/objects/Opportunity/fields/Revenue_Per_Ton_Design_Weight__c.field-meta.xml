<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_Per_Ton_Design_Weight__c</fullName>
    <description>Price Ex Works multiplied by quote tonnage</description>
    <formula>IF(  Actual_Tonnage__c =0,
    0, 
    Amount *   Actual_Tonnage__c 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Price Ex Works multiplied by quote tonnage</inlineHelpText>
    <label>Total Revenue - Ex Works (Ton)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
