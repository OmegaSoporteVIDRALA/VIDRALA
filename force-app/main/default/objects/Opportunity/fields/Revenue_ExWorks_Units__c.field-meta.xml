<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_ExWorks_Units__c</fullName>
    <description>Price Ex Works multiplied by quote volume</description>
    <externalId>false</externalId>
    <formula>IF(  Actual_Volume__c
=0,
    0, 
    Amount * Actual_Volume__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Price Ex Works multiplied by quote volume</inlineHelpText>
    <label>Total Revenue - Ex Works (Units)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
