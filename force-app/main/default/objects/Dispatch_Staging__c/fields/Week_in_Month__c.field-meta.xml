<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Week_in_Month__c</fullName>
    <description>Week in month, starting with week containing first Monday</description>
    <externalId>false</externalId>
    <formula>CEILING( ( Planned_Delivery_Date__c - First_Monday_of_Month__c + 1) / 7)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Week in Month</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
