<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>On_Time_Loads__c</fullName>
    <description>Load delivered on time and in full:  1 or 0</description>
    <externalId>false</externalId>
    <formula>IF(AND(Delivery_Status__c == &quot;On Time&quot;, NOT(Collapsed__c)), 1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>OTIF Loads</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
