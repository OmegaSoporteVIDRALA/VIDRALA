<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Remaining_Tickets__c</fullName>
    <description>Calculates how many tickets are still available</description>
    <externalId>false</externalId>
    <formula>Number_Of_Tickets__c - External_Accepted__c - External_Invited__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>How many tickets are still available after acceptances and still pending invitations</inlineHelpText>
    <label>Remaining Tickets</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
